<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="560.0" style="-fx-background-color: #9A7E7E#9A7E7E;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.Explore">
   <children>
      <Label fx:id="itemsLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="360.0" layoutY="20.0" prefHeight="60.0" prefWidth="120.0" text="Items">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label fx:id="actionsLabel" alignment="CENTER" layoutX="50.0" layoutY="20.0" prefHeight="60.0" prefWidth="120.0" text="Actions">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Button fx:id="cutTreeButton" layoutX="75.0" layoutY="110.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="75.0" style="-fx-background-color: #552A2A#552A2A;" text="Cut Tree" textFill="WHITE" />
      <Button fx:id="getWaterButton" layoutX="75.0" layoutY="150.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="75.0" style="-fx-background-color: #552A2A#552A2A;" text="Get Water" textFill="WHITE" />
      <Button fx:id="miningButton" layoutX="75.0" layoutY="190.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="75.0" style="-fx-background-color: #552A2A#552A2A;" text="Mining" textFill="WHITE" />
      <ImageView fx:id="itemImage1" fitHeight="40.0" fitWidth="40.0" layoutX="400.0" layoutY="90.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="itemImage2" fitHeight="40.0" fitWidth="40.0" layoutX="400.0" layoutY="140.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="itemImage3" fitHeight="40.0" fitWidth="40.0" layoutX="400.0" layoutY="190.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="homeButton" layoutX="160.0" layoutY="310.0" mnemonicParsing="false" onAction="#homeButton" prefHeight="25.0" prefWidth="75.0" style="-fx-background-color: #552A2A#552A2A;" text="Home" textFill="WHITE" />
      <Button fx:id="exploreButton" layoutX="330.0" layoutY="310.0" mnemonicParsing="false" onAction="#exploreButton" prefHeight="25.0" prefWidth="75.0" style="-fx-background-color: #552A2A#552A2A;" text="Explore" textFill="WHITE" />
   </children>
</AnchorPane>
